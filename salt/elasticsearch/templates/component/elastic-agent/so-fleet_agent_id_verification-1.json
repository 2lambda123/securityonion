{
        "template": {
          "settings": {
            "analysis": {
        "analyzer": {
          "es_security_analyzer": {
            "type": "custom",
            "char_filter": [
              "whitespace_no_way"
            ],
            "filter": [
              "lowercase",
              "trim"
            ],
            "tokenizer": "keyword"
          }
        },
        "char_filter": {
          "whitespace_no_way": {
            "type": "pattern_replace",
            "pattern": "(\\s)+",
            "replacement": "$1"
          }
        },
        "filter": {
          "path_hierarchy_pattern_filter": {
            "type": "pattern_capture",
            "preserve_original": true,
            "patterns": [
              "((?:[^\\\\]*\\\\)*)(.*)",
              "((?:[^/]*/)*)(.*)"
            ]
          }
        },
        "tokenizer": {
            "path_tokenizer": {
              "type": "path_hierarchy",
              "delimiter": "\\"
            }
          }
            },
            "index": {
              "final_pipeline": ".fleet_final_pipeline-1"
            }
          },
          "mappings": {
            "properties": {
              "event": {
                "properties": {
                  "agent_id_status": {
                    "ignore_above": 1024,
                    "type": "keyword"
                  },
                  "ingested": {
                    "format": "strict_date_time_no_millis||strict_date_optional_time||epoch_millis",
                    "type": "date"
                  }
                }
              }
            }
          }
        },
        "_meta": {
          "managed_by": "fleet",
          "managed": true
        }
      }
